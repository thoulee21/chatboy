{
  "expo": {
    "name": "Chatboy",
    "slug": "chatboy",
    "scheme": "chatboy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thoulee.chatboy"
    },
    "web": {
      "favicon": "./src/assets/images/icon.png",
      "bundler": "metro"
    },
    "updates": {
      "enabled": true,
      "url": "https://u.expo.dev/afb15eb7-2e38-4f5d-b028-de4b8c562c23"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "./plugin/gradle-properties",
      "expo-router",
      "expo-updates",
      "expo-system-ui",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#6a2c73"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "afb15eb7-2e38-4f5d-b028-de4b8c562c23"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
