{
  "expo": {
    "name": "chatboy",
    "slug": "chatboy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thoulee.chatboy"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "updates": {
      "enabled": true
    }
  }
}
